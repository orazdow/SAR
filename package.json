  "main": "index.js",
  "scripts": {
    "lint": "tslint --format stylish --project tsconfig.json --config tslint.json --fix",
    "build": "npm run lint && node build.js > bundle.js",
    "watch": "rum . bundle.js -p 8000 --  --debug index.js",
    "data-bundle": "browserify -r ./sar_tree.json | terser -c -m > data.js",
    "fetch": "node fetch.js tree > sar_tree.json && npm run data-bundle",
    "list": "node fetch list",
    "prod": "npm run lint && node build.js | terser -m -c > bundle.js"
  },
