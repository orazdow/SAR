    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@types/react": "^16.9.15",
    "@types/react-dom": "^16.9.4",
    "browserify": "^16.5.0",
    "rum": "^5.1.0",
    "terser": "^4.4.2",
    "tsify": "^4.0.1",
    "tslint": "^5.20.1",
    "typescript": "^3.7.3"
  }
